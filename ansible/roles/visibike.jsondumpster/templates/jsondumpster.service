[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User={{ jsondumpster_user }}
Group={{ jsondumpster_www_group }}
WorkingDirectory={{ jsondumpster_path }}
ExecStart={{ jsondumpster_venv }}/bin/gunicorn --workers 1 --bind {{ jsondumpster_socket}} jsondumpster.wsgi:application

[Install]
WantedBy=multi-user.target